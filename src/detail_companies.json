{
    
    "$id":"detail_companies.json",
    "type":"object",
    "required":["data","message"],
    "properties":{
        "data":[{
          "_id" :{
            "type":"string",
            "pattern":"^[1-9][0-9]*$" 
          },
          "name":{
            "type":"string"
        },
        "address":{
            "type":"string"
         },
         "balance":{
            "type":"float64"
        },
        "loyaltyProgram":{
            "type":"float64"
        },
        "active":{
            "type":"boolean"
        },
        "postpaid":{
            "type":"boolean"
        },
        "createdAt":{
            "type":"string",
            "format":"date-time"
        },
        "updatedAt":{
            "type":"string",
           "format":"date-time"
        }
    }],
    "message":{
        "type":"string"
    }
    }
}
