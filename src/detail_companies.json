{
    "$id": "detail_companies.json",
    "type": "object",
    "required": [
        "_id",
        "name",
        "address",
        "balance",
        "cashbackPercent",
        "active",
        "paidType",
        "createdAt",
        "updatedAt"
    ],
    "properties": {
        "_id": {
            "type": "string",
            "pattern": "^[1-9][0-9]*$"
        },
        "name": {
            "type": "string"
        },
        "address": {
            "type": "string"
        },
        "balance": {
            "type": "number"
        },
        "cashbackPercent": {
            "type": "number"
        },
        "active": {
            "type": "boolean"
        },
        "paidType": {
            "type": "string"
        },
        "createdAt": {
            "type": "string",
            "format": "date-time"
        },
        "updatedAt": {
            "type": "string",
            "format": "date-time"
        }
    }
}